@model Model.City

@{
    ViewData["Title"] = "Details";
}
<br />
<h1>Detalhes Cidade @Model.NameCity</h1>

<div>
    <h4>Informações</h4>
    <hr />
    <br />
    <dl class="row">
        <dt class = "col-sm-2">
            Nome Cidade
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NameCity)
        </dd>
        <dt class = "col-sm-2">
            Estado
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.State)
        </dd>
    </dl>
</div>
<hr />
<div>
    <a asp-action="Index" class="btn btn-outline-secondary">Retornar</a>
</div>
